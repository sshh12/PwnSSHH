
config wifi-device 'radio0'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/ar933x_wmac'
        option htmode 'HT20'
        option disabled '0'

config wifi-iface
        option device 'radio0'
        option network wan
        option mode sta
        option ssid 'WIFI-SSID'
        option key 'WIFI-PASS'
        option encryption 'psk2'
